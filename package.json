{
  "name": "parcel_js_esri_preact_js",
  "version": "1.0.0",
  "description": "Parcel JS - Esri API Preact JS",
  "main": "App.jsx",
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "build": "rm -rf ./build && parcel build ./src/index.html --out-dir build --public-url ./ --detailed-report",
    "build:scope-hoisting": "rm -rf ./build && parcel build ./src/index.html --out-dir build --public-url ./  --experimental-scope-hoisting --detailed-report"
  },
  "keywords": [],
  "author": "Bojan Zivkovic",
  "license": "MIT",
  "dependencies": {
    "esri-loader": "^2.9.2",
    "preact": "^8.4.2",
    "preact-compat": "^3.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-preset-preact": "^1.1.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.22.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
